# RoboSim Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# SUPABASE CONFIGURATION (Required for authentication)
# =============================================================================
# Get these from your Supabase project: https://supabase.com
# Go to Project Settings > API to find these values

VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# =============================================================================
# OPTIONAL: AI SERVICES
# =============================================================================

# OpenAI API Key (for AI chat features)
# VITE_OPENAI_API_KEY=sk-...

# Anthropic API Key (for Claude-based features)
# VITE_ANTHROPIC_API_KEY=sk-ant-...

# fal.ai API Key (for Image-to-3D conversion)
# VITE_FAL_API_KEY=...

# =============================================================================
# OPTIONAL: HUGGING FACE
# =============================================================================

# HuggingFace token (for dataset uploads)
# VITE_HUGGINGFACE_TOKEN=hf_...

# =============================================================================
# DEVELOPMENT
# =============================================================================

# Set to 'true' to enable development features
# VITE_DEV_MODE=true
